#include <stdio.h>
#include <stdlib.h>
int main()
{
    int num, *arr, i;
    scanf("%d", &num);
    arr = (int*) malloc(num * sizeof(int));
    for(i = 0; i < num; i++) {
        scanf("%d", arr + i);
}
/* Write the logic to reverse the array. */
int c=num/2, temp, k=0, j=num-1;
while(c>0)
{
       temp=*(arr+k);
       *(arr+k)=*(arr+j);
       *(arr+j)=temp;
       k++;
       j--;
       c--;
}
for(i = 0; i < num; i++)
printf("%d ", *(arr + i));
return 0;
}
